{
    "train_dataset": {
        "images_dir": "train2017",
        "annotations_file": "annotations/person_keypoints_train2017_modified.json",
        "category_name": "person",
        "image_shape": [256, 256],
        "target_shape": [32, 32],
        "stdev": 0.025,
        "is_bmp": false,
        "random_angle": [-3.14, 3.14],
        "random_scale": [0.8, 1.2],
        "random_translate": [-0.2, 0.2]
    },
    "stdev_schedule": {
        "0": 0.025
    },
    "color_jitter": {
        "brightness": 0.25,
        "contrast": 0.25,
        "saturation": 0.25,
        "hue": 0.1
    },
    "test_dataset": {
        "images_dir": "val2017",
        "annotations_file": "annotations/person_keypoints_val2017_modified.json",
        "category_name": "person",
        "image_shape": [256, 256],
        "target_shape": [32, 32],
        "stdev": 0.025,
        "is_bmp": false,
        "random_angle": [-0.0, 0.0],
        "random_scale": [1.0, 1.0],
        "random_translate": [-0.0, 0.0]
    },
    "train_loader": {
        "batch_size": 64,
        "shuffle": true,
        "num_workers": 8,
        "pin_memory": true
    },
    "test_loader": {
        "batch_size": 64,
        "shuffle": true,
        "num_workers": 8,
        "pin_memory": true
    },
    "model": {
        "name": "resnet18_multiscale_stride8",
        "kwargs": {
            "cmap_channels": 18,
            "paf_channels": 42,
            "num_expansion_channels": 1024,
            "num_mix_channels": 128,
            "num_mix_layers": 1
        }
    },
    "optimizer": {
        "name": "Adam",
        "kwargs": {
            "lr": 1e-3
        }
    },
    "epochs": 250,
    "lr_schedule": {
        "0": 0.001,
        "75": 0.0001,
        "150": 0.00001
    },
    "checkpoints": {
        "interval": 10
    }
}