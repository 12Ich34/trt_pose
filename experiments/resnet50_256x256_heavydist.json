{
    "train_dataset": {
        "images_dir": "train2017",
        "annotations_file": "annotations/person_keypoints_train2017_modified.json",
        "category_name": "person",
        "image_shape": [256, 256],
        "target_shape": [64, 64],
        "stdev": 0.023,
        "is_bmp": false,
        "random_angle": 0.1,
        "random_scale": 0.2,
        "random_translate": 0.1
    },
    "color_jitter": {
        "brightness": 0.3,
        "contrast": 0.3,
        "saturation": 0.3,
        "hue": 0.05
    },
    "test_dataset": {
        "images_dir": "val2017",
        "annotations_file": "annotations/person_keypoints_val2017_modified.json",
        "category_name": "person",
        "image_shape": [256, 256],
        "target_shape": [64, 64],
        "stdev": 0.023,
        "is_bmp": false,
        "random_angle": 0.0,
        "random_scale": 0.0,
        "random_translate": 0.0
    },
    "train_loader": {
        "batch_size": 128,
        "shuffle": true,
        "num_workers": 8,
        "pin_memory": true
    },
    "test_loader": {
        "batch_size": 128,
        "shuffle": true,
        "num_workers": 8,
        "pin_memory": true
    },
    "model": {
        "name": "resnet50_baseline",
        "kwargs": {
            "cmap_channels": 18,
            "paf_channels": 42,
            "upsample_channels": 256,
            "num_upsample": 3
        }
    },
    "optimizer": {
        "name": "Adam",
        "kwargs": {
            "lr": 0.001
        }
    },
    "epochs": 200,
    "lr_schedule": {
        "0": 0.001,
        "75": 0.0001,
        "150": 0.00001
    },
    "checkpoints": {
        "interval": 3,
        "directory": "resnet50_256x256_coco_heavydist"
    },
    "logfile": "resnet50_256x256_heavydist.log"
}